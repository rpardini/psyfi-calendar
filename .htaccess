<FilesMatch "\.(gif|jpe?g|png|webp)$">
	Header set WTF image
	Header set "Cache-Control" "public, max-age=31536000"
</FilesMatch>

<FilesMatch "\.(css)$">
	Header set WTF css
	Header set Cache-Control "public, max-age=31536000"
</FilesMatch>

<FilesMatch "\.(js)$">
	Header set WTF javascript
	Header set Cache-Control "public, max-age=31536000"
</FilesMatch>
